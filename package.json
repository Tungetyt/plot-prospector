    "leaflet-geosearch": "^3.11.0",
    "libphonenumber-js": "^1.10.51",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "next-intl": "3.1.2",
    "pino": "^8.16.2",
    "prisma": "^5.6.0",
    "proj4": "^2.9.2",
    "react": "18.2.0",
    "react-currency-input-field": "^3.6.12",
