{
  "name": "plot-prospector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker:compose": "docker-compose up -d --build",
    "docker:compose-prod": "docker compose -f \"docker-compose.prod.yml\" up -d --build",
    "docker:prune": "docker system prune -a",
    "docker:kill-em-all": "docker rm -f $(docker ps -a -q); docker rmi -f $(docker images -a -q); docker volume rm $(docker volume ls -q)",
    "prisma:migrate": "docker-compose exec next-js npx prisma migrate dev && npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.0.0",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "next-auth": "^4.22.3",
    "postcss": "8.4.27",
    "prisma": "^5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}