{
  "name": "plot-prospector",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker:build": "docker-compose up -d --build",
    "docker:build-prod": "docker compose -f \"docker-compose.prod.yml\" up -d --build",
    "prisma:migrate": "docker-compose exec next-js npx prisma migrate dev && npx prisma generate",
    "prisma:studio": "docker-compose exec next-js npx prisma studio",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "pnpm exec prettier . --write",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.1.1",
    "@types/node": "20.5.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "daisyui": "^3.5.1",
    "eslint-config-next": "13.4.16",
    "immer": "^10.0.2",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "leaflet-geosearch": "^3.8.0",
    "next": "13.4.16",
    "next-auth": "^4.23.0",
    "next-intl": "3.0.0-beta.9",
    "pino": "^8.15.0",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-leaflet": "^4.2.1",
    "react-phone-number-input": "^3.3.2",
    "tailwindcss": "3.3.3",
    "vite-tsconfig-paths": "^4.2.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.1",
    "prisma": "^5.1.1"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/leaflet": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-istanbul": "^0.34.1",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "typescript": "5.1.6",
    "vitest": "^0.34.1"
  }
